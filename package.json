{
  "name": "px-egg-simple",
  "version": "1.0.0",
  "description": "You know, for silence",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "egg": "^3",
    "egg-cors": "^2.2.3",
    "egg-mysql": "^4.0.0",
    "egg-redis": "^2.5.0",
    "egg-scripts": "^2",
    "egg-socket.io": "^4.1.6",
    "egg-static": "^2.3.1",
    "egg-validate": "^2.0.2",
    "jsonwebtoken": "^9.0.2",
    "minio": "^7.1.3"
  },
  "devDependencies": {
    "dotenv": "^16.3.1",
    "egg-bin": "^5",
    "egg-ci": "^2",
    "egg-mock": "^5",
    "eslint": "^8",
    "eslint-config-egg": "^12"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --port=9999 --daemon --title=egg-server-px-egg-simple",
